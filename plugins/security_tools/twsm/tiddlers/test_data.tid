echarts-refresh-trigger: [twsm_class[theme]twsm_security_score[]twsm_json_field:score[]]

\define indicatorPrefix() { "text": "
\define indicatorSuffix() ", "max": 100}

{
"legend": {
    "right": "10%",
    "top": "3%"
  },
  "color": ["#0000FF"],
  "radar": [
    {
      "shape": "circle",
      "indicator": [
<$list filter="[twsm_class[theme]addprefix<indicatorPrefix>addsuffix<indicatorSuffix>join[,]]">
<<currentTiddler>>
</$list>        
      ],
      "center": ["50%", "50%"],
      "radius": 190,
      "axisName": {
        "color": "#fff",
        "backgroundColor": "#666",
        "borderRadius": 3,
        "padding": [3, 5]
      }
    }
  ],
  "series": [
    {
      "type": "radar",
      "data": [
        {
          "value": [
<$list filter="[twsm_class[theme]twsm_security_score[]twsm_json_field:score[]join[,]]">
<<currentTiddler>>
</$list>
            ],
          "name": "Score (0 = bad, 100 = good)",
          "symbol": "circle",
          "symbolSize": 12,
          "lineStyle": {
            "type": "dashed"
          },
          "areaStyle": {
            "color": "#A0A0CAA0"
          },
          "label": {
            "show": true
          }
        }
      ]
    }
  ]
}